{
  "extends": ["config:base"],
  "packageRules": [
    {
      "matchPackagePrefixes": ["eframe","egui","ehttp"],
      "groupName": "egui"
    },
    {
      "matchUpdateTypes": ["minor", "pin", "major"],
      "automergeType": "branch",
      "automerge": true
    },
    {
      "matchUpdateTypes": ["patch", "digest"],
      "enabled": false
    }
  ]
}
